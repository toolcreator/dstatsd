    "dstatsd": "dist/main.js"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "start": "dist/main.js",
    "docker-build": "docker build --tag toolcreator/dstatsd ."
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
